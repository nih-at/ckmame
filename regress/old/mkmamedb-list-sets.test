description list all configured sets using mkmamedb
return 0
args --list-sets
program mkmamedb
file-data .ckmamerc
[global]
sets = [ "standard set 1", "standard set 2" ]
["non-standard set 1"]
verbose = true
["non-standard set 2"]
report-missing = false
end-of-data
stdout-data
non-standard set 1
non-standard set 2
standard set 1
standard set 2
end-of-data
