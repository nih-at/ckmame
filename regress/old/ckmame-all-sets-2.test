description list all configured sets, remove files from extra
return 0
args --all-sets  -Fj -v 1-4 1-8
file-new roms1/1-4.zip 1-4-ok.zip
file-new roms2/1-8.zip 1-8-ok.zip
file-del extra/1-4.zip 1-4-ok.zip
file-del extra2/1-8.zip 1-8-ok.zip
file-data .ckmamerc
[global]
report-correct = true
["non-standard set 1"]
rom-directory = "roms1"
extra-directories = [ "extra" ]
["non-standard set 2"]
rom-directory = "roms2"
extra-directories = [ "extra", "extra2" ]
end-of-data
